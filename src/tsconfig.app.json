{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "skipLibCheck": true,
    "emitDecoratorMetadata":true,
    "outDir": "../out-tsc/app",
    "baseUrl": "./",
    "module": "esnext",
    "target": "es5",
    "types": [],
    "paths": {
      "@angular/*": [
        "../node_modules/@angular/*"
      ]
    }
  },
  "angularCompilerOptions": {
    "disableTypeScriptVersionCheck": true,
   
    "strictInjectionParameters": true,
    "allowSyntheticDefaultImports": true,
   
    "enableIvy": false
  },
  "exclude": [
    "test.ts",
    "**/*.spec.ts"
  ]
}
